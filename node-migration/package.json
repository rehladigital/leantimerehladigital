{
  "name": "leantimerehladigital",
  "version": "1.0.0",
  "description": "Node migration runtime for Al Mudheer",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:generate:mysql": "prisma generate --schema prisma/schema.mysql.prisma",
    "prisma:migrate:dev": "prisma db push --schema prisma/schema.prisma",
    "prisma:push:mysql": "prisma db push --schema prisma/schema.mysql.prisma",
    "seed": "tsx prisma/seed.ts",
    "setup:local": "npm run prisma:generate && npm run prisma:migrate:dev && npm run seed",
    "setup:hostinger": "npm run prisma:generate:mysql && npm run prisma:push:mysql && npm run seed",
    "test": "echo \"No tests configured yet\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rehladigital/RehladigitalAlMudheer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/rehladigital/RehladigitalAlMudheer/issues"
  },
  "homepage": "https://github.com/rehladigital/RehladigitalAlMudheer#readme",
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "helmet": "^8.1.0",
    "jose": "^6.1.3",
    "mysql2": "^3.18.2",
    "openid-client": "^6.8.2",
    "prisma": "^6.19.2",
    "sqlite3": "^5.1.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^25.3.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
